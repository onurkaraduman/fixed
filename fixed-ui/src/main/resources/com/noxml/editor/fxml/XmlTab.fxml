            <tabs>
                <Tab text="XML">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                    prefWidth="200.0">
                            <children>
                                <XmlTreeView fx:id="treeView"
                                             AnchorPane.bottomAnchor="0.0"
                                             AnchorPane.leftAnchor="0.0"
                                             AnchorPane.rightAnchor="0.0"
                                             AnchorPane.topAnchor="0.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
